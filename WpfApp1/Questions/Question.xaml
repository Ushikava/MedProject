<UserControl x:Class="TestV.Questions.QuestoinUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TestV.Questions"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid x:Name="MainGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock Name="QuestionTextLBL" Text="Question" HorizontalAlignment="Center" Grid.Row="0"/>
        <StackPanel Name="QuestionPanel" Grid.Row="1">
            
        </StackPanel>
    </Grid>
</UserControl>
